<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no" />
    <title>@ViewData["Title"] - Asernatat_3</title>
    <link href="~/css/site.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <style>
        body {
            background-image: url("../img/miniFon.jpg");
            background-color: #364534;
        }

        .footer-classic, .my-color {
            color: #ffffff;
            background: linear-gradient(to right, #1a423f, #1d2c2c);
        }

        .myy-colorr {
            background: linear-gradient(to right, #364534, #2c3f2a);
        }

        ul, ol {
            list-style: none;
            padding: 0;
            margin: 0;
        }

        .social-inner {
            display: flex;
            flex-direction: column;
            align-items: center;
            width: 100%;
            padding: 23px;
            font: 900 13px/1 "Lato", -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif;
            text-transform: uppercase;
            color: #000000;
        }

        .social-container .col {
            border: 1px solid #315752;
        }

        .col-my {
            color: #000000;
            border-color: #000000;
        }

            .col-my:hover {
                color: #57fbeb;
                background-color: #1a3231;
                border-color: #57fbeb;
                transition: .59s ease;
            }

            .col-my:focus {
                box-shadow: 0 0 0 0.2rem rgba(064, 250, 233, 0.5);
            }

            .col-my:disabled {
                color: #000000;
                background-color: transparent;
            }

            .col-my:not(:disabled):active {
                color: #000000;
                background-color: #315752;
                border-color: #57fbeb;
            }

                .col-my:not(:disabled):active:focus {
                    box-shadow: 0 0 0 0.2rem rgba(064, 250, 233, 0.5);
                }

        .color-links {
            color: #000000 !important;
        }

            .color-links:visited {
                color: #000000;
            }

            .color-links:hover {
                color: #57fbeb !important;
            }

            .color-links:active {
                color: #000000;
            }

        .btn-outline-mycolor {
            color: #57fbeb;
            border-color: #57fbeb;
        }

            .btn-outline-mycolor:hover {
                color: #212529;
                background-color: #57fbeb;
                border-color: #57fbeb;
            }

            .btn-outline-mycolor.focus, .btn-outline-mycolor:focus {
                box-shadow: 0 0 0 .2rem rgba(255,193,7,.5)
            }

            .btn-outline-mycolor.disabled, .btn-outline-mycolor:disabled {
                color: #57fbeb;
                background-color: transparent;
            }

            .btn-outline-mycolor:not(:disabled):not(.disabled).active, .btn-outline-mycolor:not(:disabled):not(.disabled):active, .show > .btn-outline-mycolor.dropdown-toggle {
                color: #212529;
                background-color: #57fbeb;
                border-color: #57fbeb;
            }

                .btn-outline-mycolor:not(:disabled):not(.disabled).active:focus, .btn-outline-mycolor:not(:disabled):not(.disabled):active:focus, .show > .btn-outline-mycolor.dropdown-toggle:focus {
                    box-shadow: 0 0 0 .2rem rgba(255,193,7,.5)
                }

    </style>
</head>
<body>
    <a name="base"></a>
    <header class=" sticky-top">
        <nav class="navbar navbar-expand-lg navbar-light sticky-top my-color" style="border-bottom: 1px solid black;">
            <a class="navbar-brand b" asp-area="" asp-controller="Home" asp-action="Index">
                <img src="#" class="rounded" width="100" height="60" alt="">
            </a>
            <button class="navbar-toggler btn btn-outline-mycolor" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse justify-content-end" id="navbarSupportedContent">
                <div class="row justify-content-end mr-5">
                    <ul class="navbar-nav mr-auto">
                        <li class="nav-item px-3 active">
                            <a class="nav-link color-links" asp-area="" asp-controller="Home" asp-action="Index">
                                Home
                                <span class="sr-only">
                                    (current)
                                </span>
                            </a>
                        </li>
                        <li class="nav-item px-3">
                            <a class="nav-link color-links" asp-area="" asp-controller="" asp-action="">
                                Theme
                            </a>
                        </li>
                        <li class="nav-item px-3">
                            <a class="nav-link color-links" asp-area="" asp-controller="" asp-action="">
                                To order
                            </a>
                        </li>
                        <li class="nav-item px-3">
                            <partial name="_LoginPartial" />
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <div class="container-fluid" style="min-height: 600px;">
        <main role="main" class="row" style="position: center">
            <div class="col-md-6 col-xl-4 a2"></div>
            <div class="col-md-6 col-xl-4 a3 myy-colorr" style="min-height: 600px;border: 1px solid black;">
                @RenderBody()
            </div>
            <div class="col-md-6 col-xl-4 a4"></div>
        </main>
    </div>

    <footer class="section footer-classic context-dark text-black" style="border-top: 1px solid black;">
        <div class="container">
            <div class="row row-30 pt-3">
                <div class="col-md-5 col-xl-6">
                    <div class="pr-xl-4">
                        <a class="brand" href="#base"> <img class="brand-logo-light" src="#" alt="" width="140" height="37"> </a>
                        <p>Text</p>
                        <!-- Rights-->
                        <p class="rights">
                            <span>©  </span><span class="copyright-year">2020</span><span> </span><span>Waves</span><span>. </span><span>All Rights Reserved.</span><br>
                            &copy; 2020 - Asernatat_3 - <a class="color-links" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </p>
                    </div>
                </div>
                <div class="col-md-6">
                    <h5>Contacts</h5>
                    <dl class="contact-list">
                        <dt>Address:</dt>
                        <dd>exampl_address</dd>
                    </dl>
                    <dl class="contact-list">
                        <dt>email:</dt>
                        <dd><a class="color-links" href="mailto:#">exampl_e-mail@mail.ru</a></dd>
                    </dl>
                    <dl class="contact-list">
                        <dt>phones:</dt>
                        <dd>
                            <a class="color-links" href="tel:#">exampl_phone1</a> <span><br></span> <a class="color-links" href="tel:#">exampl_phone2</a>
                        </dd>
                    </dl>
                </div>
            </div>
        </div>
        <div class="row no-gutters social-container">
            <div class="col"><a class="social-inner col-my" href="#"><span class="icon mdi mdi-facebook"></span><span>Facebook</span></a></div>
            <div class="col"><a class="social-inner col-my" href="#"><span class="icon mdi mdi-instagram"></span><span>instagram</span></a></div>
            <div class="col"><a class="social-inner col-my" href="#"><span class="icon mdi mdi-twitter"></span><span>twitter</span></a></div>
            <div class="col"><a class="social-inner col-my" href="#"><span class="icon mdi mdi-youtube-play"></span><span>google</span></a></div>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
